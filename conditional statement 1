#include<stdio.h>
int main ()
{
	float s1,s2,s3,s4,s5,per;
	
	printf("enter marks of 5 subjects\n");
	scanf("%f %f %f %f %f",&s1,&s2,&s3,&s4,&s5);
	
	per= (s1+s2+s3+s4+s5)/5.0;
	
	if(per>=85)
	{
		printf("Grade A\n");
	}
	else
	{
		if(per>=70){
			printf("Grade B\n");
		}
		else
		{
			if(per>=55){
				printf("Grade C\n");
			}
			else
			{
				if(per>=40){
					printf("Grade D\n");
				}
			    else
			    {
				if(per<40){
					printf("Grade F\n");
				}
			}
		
		
		}
	}
	
	
}
return 0;
}
